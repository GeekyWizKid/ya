开始
    数组 数列 = [64, 34, 25, 12, 22, 11, 90]
    显示("原始数组：")
    显示(数列)

    长度 = 7
    外循环 = 0
    当(外循环 < 长度 - 1) 开始
        内循环 = 0
        当(内循环 < 长度 - 外循环 - 1) 开始
            如果(数列[内循环] > 数列[内循环 + 1]) 开始
                临时 = 数列[内循环]
                数列[内循环] = 数列[内循环 + 1]
                数列[内循环 + 1] = 临时
            结束
            内循环 = 内循环 + 1
        结束
        外循环 = 外循环 + 1
    结束

    显示("排序后数组：")
    显示(数列)
结束 